{
  "name": "your-name-here/Intervention",
  "description": "Intervention plugin for CakePHP",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "cakephp/cakephp": "^3.4",
    "intervention/image": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Intervention\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Intervention\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
